{
  "name": "seadex",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "dev:backend": "cd ../pb && modd",
    "backend": "cd ../pb/ && pocketbase serve --publicDir=../sk/build",
    "build": "vite build",
    "build:backend": "cd ../pb && go build",
    "typegen": "pocketbase-typegen --db ../pb/pb_data/data.db --out ./src/lib/pocketbase/generated-types.ts",
    "test": "playwright test",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "format": "eslint . --fix"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/adapter-static": "^2.0.2",
    "@sveltejs/kit": "^1.22.3",
    "@types/node": "^20.4.2",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.45.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-svelte": "^2.32.2",
    "pocketbase-typegen": "^1.1.11",
    "postcss": "^8.4.26",
    "quartermoon": "^1.2.3",
    "sass": "^1.63.6",
    "svelte": "^4.0.5",
    "svelte-check": "^3.4.6",
    "svelte-eslint-parser": "^0.32.1",
    "svelte-preprocess": "^5.0.4",
    "tslib": "^2.6.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.4"
  },
  "type": "module",
  "dependencies": {
    "anitomyscript": "github:ThaUnknown/anitomyscript",
    "highlight.js": "^11.9.0",
    "parse-torrent": "^11.0.12",
    "path-esm": "^1.0.0",
    "pocketbase": "^0.18.3",
    "svelte-sonner": "^0.1.2"
  }
